{
  "name": "myapp",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "graphql:generate": "cross-env DEBUG=1 graphql-codegen --config graphql/codegen.yml",
    "generate": "pnpm run graphql:generate",
    "generate:all": "pnpm run --recursive --if-present generate",
    "vitest": "vitest",
    "test": "pnpm run vitest",
    "test:ci": "pnpm run migrate:test && pnpm run vitest",
    "lint": "eslint --ext .js,.ts,.tsx src stacks graphql/documents",
    "lint:fix": "pnpm run lint -- --fix",
    "build": "pnpm run -r build",
    "deploy": "pnpm graphql:generate && sst deploy",
    "deploy:main": "pnpm graphql:generate && sst deploy main",
    "remove": "sst remove",
    "prettier:exc": "./node_modules/.bin/prettier --config .prettierrc.yaml --write",
    "prettier": "pnpm run prettier:exc -- '**/*.{js,ts,tsx,css,json,yaml,yml,md}'",
    "clean": "rm -rf .build dist graphql/generated"
  },
  "dependencies": {
    "@aws-lambda-powertools/logger": "^0.8.0",
    "@aws-sdk/client-secrets-manager": "^3.67.0",
    "@serverless-stack/cli": "~1.2.11",
    "@serverless-stack/resources": "~1.2.11",
    "aws-cdk-lib": "2.24.0",
    "memoizee": "^0.4.15"
  },
  "devDependencies": {
    "@apollo/client": "^3.6.5",
    "@aws-cdk/aws-appsync-alpha": "2.24.0-alpha.0",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/schema-ast": "^2.4.1",
    "@graphql-codegen/typescript": "^2.4.11",
    "@graphql-codegen/typescript-operations": "^2.4.0",
    "@graphql-codegen/typescript-react-apollo": "*",
    "@serverless-stack/core": "~1.2.11",
    "@serverless-stack/static-site-env": "~1.2.11",
    "@tsconfig/node16-strictest-esm": "^1.0.1",
    "@types/memoizee": "^0.4.7",
    "@types/node": "<15.0.0",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "chalk": "^5.0.1",
    "constructs": "^10.1.17",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "dotenv-cli": "^5.1.0",
    "eslint": "^8.16.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "fs-extra": "^10.1.0",
    "graphql": "^16.5.0",
    "graphql-tag": "^2.12.6",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.8",
    "prettier": "^2.6.2",
    "typescript": "^4.7.2",
    "vitest": "^0.12.9"
  },
  "engines": {
    "node": ">=0.16",
    "npm": ">=7.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{yaml,yml}": [
      "yamllint"
    ],
    "{*.{ts,js},!(node*)**/*.{ts,tsx,js,jsx}}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "pnpm": {
    "enable-pre-post-scripts": true
  }
}
