#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/cyber/dev/sst-prisma/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/cyber/dev/sst-prisma/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../node_modules/.pnpm/aws-lambda@1.0.7/node_modules/aws-lambda/bin/lambda" "$@"
else
  exec node  "$basedir/../../../node_modules/.pnpm/aws-lambda@1.0.7/node_modules/aws-lambda/bin/lambda" "$@"
fi
