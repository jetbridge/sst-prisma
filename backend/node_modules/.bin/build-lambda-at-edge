#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/cyber/dev/sst-prisma/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/cyber/dev/sst-prisma/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../node_modules/.pnpm/@sls-next+lambda-at-edge@3.7.0_3zayg56kp43ruzwqmayatxilde/node_modules/@sls-next/lambda-at-edge/dist/command.js" "$@"
else
  exec node  "$basedir/../../../node_modules/.pnpm/@sls-next+lambda-at-edge@3.7.0_3zayg56kp43ruzwqmayatxilde/node_modules/@sls-next/lambda-at-edge/dist/command.js" "$@"
fi
